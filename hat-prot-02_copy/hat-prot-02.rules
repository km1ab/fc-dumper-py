    ADR3 ADR2 CLK CLR ADR1 ADR0 "unconnected-(J1-PadA15)" "unconnected-(J1-PadA18)"
    DA0 DA1 DA2 DA3 ADR12 ADR13 ADR14 DA7
    DA6 DA5 DA4 "unconnected-(J1-PadB15)" "unconnected-(J1-PadB16)" "unconnected-(J1-PadB18)" ADR15 "unconnected-(U4-Pad1)"
    "unconnected-(U4-Pad2)" "unconnected-(U4-Pad3)" "unconnected-(U4-Pad4)" "unconnected-(U4-Pad12)" PWM0 PWM1 "EIO_2" "EIO_1"
    "EIO_0" OE CS WE RST "unconnected-(U8-Pad3)" "unconnected-(U4-Pad13)" "unconnected-(U4-Pad14)"
    "unconnected-(U4-Pad15)"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule "kicad_default")
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
