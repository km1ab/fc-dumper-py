    "EIO_2" "EIO_1" "EIO_0" OE CS WE PPURD "unconnected-(SW1-Pad3)"
    "unconnected-(U2-Pad1)" "unconnected-(U2-Pad2)" "unconnected-(U2-Pad3)" "unconnected-(U2-Pad4)" "unconnected-(U2-Pad12)" "unconnected-(U2-Pad13)" "unconnected-(U2-Pad14)" "unconnected-(U2-Pad15)"
    PPUWR EXT "EIO_4" "EIO_3" /PPU{slash}A13
    (clearance_class default)
    (via_rule default)
    (rule
